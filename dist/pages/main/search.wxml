
  <view class="main-search">
    <block  wx:if="{{items1}}">
      
  <view class="checkbox-button">
    <view  wx:if="{{$checkboxButton1$hasTitle}}" class="checkbox-title">{{$checkboxButton1$title}}</view>
    <checkbox-group style="margin-right: {{$checkboxButton1$style_gutter_edge}}" bindchange="$checkboxButton1$checkboxChange">
      <view class="checkbox-wrapper"  wx:for="{{$checkboxButton1$list}}" wx:key="index" style="width:{{$checkboxButton1$style_width}};padding-right: {{$checkboxButton1$style_gutter}};margin-bottom: {{$checkboxButton1$style_gutter}}">
        <checkbox id="{{$checkboxButton1$unique}}-{{index}}" hidden value="{{item.value}}" checked="{{item.checked}}"/>
        <label for="{{$checkboxButton1$unique}}-{{index}}" class="checkbox-item{{item.checked ? '-checked' : '' }}">
          <text>{{item.name}}</text>
        </label>
      </view>
    </checkbox-group>
  </view>

    </block>
